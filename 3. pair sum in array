#include <stdio.h>
void printpairs(int arr[],int n){
    for(int i=0;i<n;i++){
        int current=arr[i];
        for(int j=i+1;j<n;j++){
            printf("(%d,%d)",current,arr[j]);
            //fflush(stdout);
        }
        printf("\n");
    }
}
int main()
{
    int arr[]={1,2,3,4,5,6};
    int size= 6;
    printpairs(arr,size);
    return 0;
}
