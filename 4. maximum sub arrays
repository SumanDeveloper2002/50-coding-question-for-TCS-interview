#include <stdio.h>
void subarray(int arr[],int n){
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            for(int k=i;k<j;k++){
              printf("%d ",arr[k]);  
            }
            printf("\n");
        }
        printf("\n");
    }
}
int main()
{
    int arr[]={1,2,3,4,5,6};
    int size= 6;
    subarray(arr,size);
    return 0;
}
